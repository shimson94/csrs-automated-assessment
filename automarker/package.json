{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "flask-dev": "FLASK_DEBUG=1 pip3 install -r ../requirements.txt && python3 -m flask --app ../src/api/app run -p 5000 --reload",
    "next-dev": "next dev",
    "open-browser": "open http://localhost:3000",
    "dev": "concurrently \"npm run next-dev\" \"npm run flask-dev\" \"npm run open-browser\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "ci": "npm run type-check && npm run lint && npm run test && npm run build"
  },
  "dependencies": {
    "@testing-library/react": "^16.1.0",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "next": "15.0.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^8",
    "eslint-config-next": "15.0.4",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
